#include <stdio.h>
int main(){
	int A, B, soma,sub,divi,multi;
    printf("\nDigite um valor de A:");
    scanf("%d",&A);
    printf("\nDigite um valor de B:");
    scanf("%d",&B);
    soma = A + B;
    sub = A - B;
    multi = A * B;
    divi = A / B;
    printf("O resultado da soma e:%d\n",soma);
    printf("O resultado da subtracao e:%d\n",sub);
    printf("O resultado da multi e:%d\n",multi);
    printf("O resultado da subtracao e:%d\n",divi);
}
